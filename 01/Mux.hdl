/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=notsel);
    Nand(a=a,b=notsel,out=AnoSel);
    Nand(a=b,b=sel,out=BSel);
    Nand(a=AnoSel,b=BSel,out=out);
}